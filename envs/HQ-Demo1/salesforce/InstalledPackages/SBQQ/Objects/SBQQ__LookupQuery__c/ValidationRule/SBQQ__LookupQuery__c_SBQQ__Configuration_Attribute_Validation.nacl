salesforce.ValidationRule SBQQ__LookupQuery__c_SBQQ__Configuration_Attribute_Validation {
  fullName = "SBQQ__LookupQuery__c.SBQQ__Configuration_Attribute_Validation"
  active = true
  errorConditionFormula = "ISPICKVAL(SBQQ__MatchType__c, 'Configuration Attribute Value') && ISBLANK(SBQQ__ProductRule__c)"
  errorDisplayField = "SBQQ__MatchType__c"
  errorMessage = "Testing Configuration Attributes is only supported for Lookup Queries associated with Product Rules."
  _parent = [
    salesforce.SBQQ__LookupQuery__c,
  ]
}
