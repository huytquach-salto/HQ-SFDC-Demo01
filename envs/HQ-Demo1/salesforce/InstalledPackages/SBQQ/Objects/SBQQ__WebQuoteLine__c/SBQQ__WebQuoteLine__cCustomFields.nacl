    apiName = "SBQQ__WebQuoteLine__c.SBQQ__EffectiveQuantity__c"
    label = "Effective Quantity"
    _required = false
    precision = 18
    scale = 2
    formula = '''
CASE(SBQQ__PricingMethod__c,
  'Block', 1,
  IF(ISPICKVAL(SBQQ__DiscountScheduleType__c, 'Slab'), 1, 
    IF(SBQQ__Existing__c, SBQQ__Quantity__c - SBQQ__PriorQuantity__c, SBQQ__Quantity__c)
  )
)
'''
    inlineHelpText = "Effective quantity used in computing totals. Same as quantity unless the product is block priced or covered by \"Slab\" discount schedule."
    createable = false
    updateable = false
    unique = false
    queryable = true
    description = "Effective quantity used in computing totals. Same as quantity unless the product is block priced or covered by \"Slab\" discount schedule."
    externalId = false
    formulaTreatBlanksAs = "BlankAsZero"
  }
